<% if current_page.last? %>
  <li class="disabled">
    <%= content_tag :a do %>
      <i class="fa-solid fa-ellipsis"></i>
    <% end %>
  </li>
<% else %>
  <li class="waves-effect2">
    <%= link_to url, :rel => 'next', :remote => remote do %>
      <i class="fa-solid fa-ellipsis"></i>
    <% end %>
  </li>
<% end %>
